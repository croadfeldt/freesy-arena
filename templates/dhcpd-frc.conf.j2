# Custom DHCP Configuration File
# Generated by Ansible Playbook

{% for subnet_info in subnets_details %}
subnet {{ subnet_info.network }} netmask 255.255.255.0 {
    range {{ subnet_info.range_start }} {{ subnet_info.range_end }};
    option routers {{ subnet_info.router }};
    # Add other options as needed
}

{% endfor %}
